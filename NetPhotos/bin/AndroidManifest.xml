<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.netphotos"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
	<uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
	<uses-permission android:name="android.permission.READ_OWNER_DATA"/>
	<uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.main.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.netphotos.NetphotosActivity"
            android:label="@string/title_activity_netphotos" >
        </activity>
        <activity
            android:name="com.example.newsListView.NewsActivity"
            android:label="@string/title_activity_news" >
        </activity>
        <activity
            android:name="com.example.dynamicloading.DynamicloadingActivity"
            android:label="@string/title_activity_dynamicloading" >
        </activity>
        <activity
            android:name="com.example.newsListView.NewsListViewActivity"
            android:label="@string/title_activity_news_list_view" >
        </activity>
        <activity
            android:name="com.example.netphotos.NewsContentActivity"
            android:label="@string/title_activity_news_content" >
        </activity>
        <activity
            android:name="com.example.cachePhotos.CachePhotosActivity"
            android:label="@string/title_activity_cache_photos" >
        </activity>
    </application>

</manifest>
